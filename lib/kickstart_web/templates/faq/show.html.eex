<h1>Show Faq</h1>

<ul>

  <li>
    <strong>Question:</strong>
    <%= @faq.question %>
  </li>

  <li>
    <strong>Answer:</strong>
    <%= @faq.answer %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.faq_path(@conn, :edit, @faq) %></span>
<span><%= link "Back", to: Routes.faq_path(@conn, :index) %></span>
